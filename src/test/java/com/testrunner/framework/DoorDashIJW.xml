<exclude name="TC08_IJW_ValidateMenuDescription"/>
<exclude name="TC09_IJW_ValidateMenuPrice"/>
<exclude name="TC90_VERIFY_ITEM_ADDTOCART_CHECKOUT"/>
<exclude name="TC91_ValidateItemIncreaseDecrease"/>
